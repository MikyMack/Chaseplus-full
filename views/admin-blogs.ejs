                    <!-- main-content -->
                    <div class="main-content">
                        <!-- main-content-wrap -->
                        <div class="main-content-inner">
                            <!-- main-content-wrap -->
                            <div class="main-content-wrap">
                                <div class="flex items-center flex-wrap justify-between gap20 mb-30">
                                    <h3> Blogs List</h3>
                                    <ul class="breadcrumbs flex items-center flex-wrap justify-start gap10">
                                        <li>
                                            <a href="/admin-dashboard"><div class="text-tiny">Dashboard</div></a>
                                        </li>
                                        <li>
                                            <i class="icon-chevron-right"></i>
                                        </li>
                                        <li>
                                            <a href="#"><div class="text-tiny">Blogs Management</div></a>
                                        </li>
                                        <li>
                                            <i class="icon-chevron-right"></i>
                                        </li>
                                        <li>
                                            <div class="text-tiny">Blogs List</div>
                                        </li>
                                    </ul>
                                </div>
                                <!-- all-attribute -->
                                <div class="wg-box">
                                    <div class="flex items-center justify-between gap10 flex-wrap">
                                        <div class="wg-filter flex-grow">
                                            <div class="show">
                                                <div class="text-tiny">Showing</div>
                                                <div class="select">
                                                    <select id="entriesPerPage">
                                                        <option value="10">10</option>
                                                        <option value="20">20</option>
                                                        <option value="30">30</option>
                                                    </select>
                                                </div>
                                                <div class="text-tiny">entries</div>
                                            </div>
                                            <form class="form-search" id="searchForm">
                                                <fieldset class="name">
                                                    <input type="text" id="searchInput" placeholder="Search here..." class="" name="search" tabindex="2">
                                                </fieldset>
                                                <div class="button-submit">
                                                    <button class="" type="submit"><i class="icon-search"></i></button>
                                                </div>
                                            </form>
                                        </div>
                                        <button class="tf-button style-1 w208" onclick="showAddForm()"><i class="icon-plus"></i>Add new</button>
                                    </div>

                                    <!-- Add/Edit Blog Form Modal -->
                                    <div id="blogFormModal" class="modal" style="display:none;">
                                        <div class="modal-content">
                                            <h3 id="modalTitle">Add New Blog</h3>
                                            <form id="blogForm" enctype="multipart/form-data">
                                                <input type="hidden" id="blogId">
                                                <div class="form-group">
                                                    <label>Title *</label>
                                                    <input type="text" name="title" id="blogTitle" required>
                                                </div>
                                                <div class="form-group">
                                                    <label>Category *</label>
                                                    <input type="text" name="category" id="blogCategory" required>
                                                </div>
                                                <div class="form-group">
                                                    <label>Date *</label>
                                                    <input type="date" name="date" id="blogDate" required>
                                                </div>
                                                <div class="form-group">
                                                    <label>Description *</label>
                                                    <textarea name="description" id="blogDescription" required></textarea>
                                                </div>
                                                <div class="form-group">
                                                    <label>Content *</label>
                                                    <textarea name="content" id="blogContent" required></textarea>
                                                </div>
                                                <div class="form-group">
                                                    <label>Image *</label>
                                                    <input type="file" name="image" id="blogImage" accept="image/*" required>
                                                </div>
                                                <div class="form-group">
                                                    <label>Meta Title</label>
                                                    <input type="text" name="metaTitle" id="blogMetaTitle">
                                                    <small>Defaults to blog title if empty</small>
                                                </div>
                                                <div class="form-group">
                                                    <label>Meta Description</label>
                                                    <textarea name="metaDescription" id="blogMetaDescription"></textarea>
                                                    <small>Defaults to first 160 characters of description if empty</small>
                                                </div>
                                                <div class="form-group">
                                                    <label>Author *</label>
                                                    <input type="text" name="author" id="blogAuthor" required>
                                                </div>
                                                <div class="form-group">
                                                    <label>
                                                        <input type="checkbox" name="isPublished" id="blogIsPublished">
                                                        Publish immediately
                                                    </label>
                                                </div>
                                                <div class="form-actions">
                                                    <button type="submit" class="tf-button style-1 mb-4">Save</button>
                                                    <button type="button" class="tf-button style-3" onclick="closeBlogForm()">Cancel</button>
                                                </div>
                                            </form>
                                        </div>
                                    </div>

                                    <div class="wg-table table-all-attribute">
                                        <ul class="table-title flex gap20 mb-14">
                                            <li>
                                                <div class="body-title">Image & Title</div>
                                            </li>    
                                            <li>
                                                <div class="body-title">Description</div>
                                            </li>
                                            <li>
                                                <div class="body-title">Views</div>
                                            </li>
                                            <li>
                                                <div class="body-title">Action</div>
                                            </li>
                                        </ul>
                                        <ul class="flex flex-column" id="blogsList">
                                            <% blogs.forEach(function(blog) { %>
                                            <li class="attribute-item item-row flex items-center justify-between gap20" data-blog-id="<%= blog._id %>">
                                                <div class="name d-flex gap-3 align-items-center">
                                                    <img class="testi-img" src="<%= blog.imageUrl %>" alt="<%= blog.title %>">
                                                    <a href="#" class="body-title-2"><%= blog.title %></a>
                                                </div>
                                                <div class="body-text truncate"><%= blog.description %></div>
                                                <div class="body-text"><%= blog.views %></div>
                                                <div class="list-icon-function">
                                                    <div class="item edit" onclick="editBlog('<%= JSON.stringify(blog) %>')">
                                                        <i class="icon-edit-3"></i>
                                                    </div>
                                                    <div class="item trash" onclick="deleteBlog('<%= blog._id %>')">
                                                        <i class="icon-trash-2"></i>
                                                    </div>
                                                    <div class="item toggle">
                                                        <label class="switch">
                                                            <input type="checkbox" 
                                                                   <%= blog.isPublished ? 'checked' : '' %>
                                                                   onchange="toggleBlogStatus('<%= blog._id %>', this)">
                                                            <span class="slider round"></span>
                                                        </label>
                                                    </div>
                                                </div>
                                            </li>
                                            <% }); %>
                                        </ul>
                                    </div>
                                    <div class="divider"></div>
                                    <div class="flex items-center justify-between flex-wrap gap10">
                                        <div class="text-tiny" id="entriesInfo">Showing <%= blogs.length %> entries</div>
                                        <ul class="wg-pagination" id="pagination">
                                            <!-- Pagination will be dynamically populated -->
                                        </ul>
                                    </div>
                                </div>

                                <script>
                                function showAddForm() {
                                    document.getElementById('modalTitle').textContent = 'Add New Blog';
                                    document.getElementById('blogForm').reset();
                                    document.getElementById('blogId').value = '';
                                    document.getElementById('blogFormModal').style.display = 'block';
                                }

                                function editBlog(blogData) {
                                    const blog = JSON.parse(blogData);
                                    document.getElementById('modalTitle').textContent = 'Edit Blog';
                                    document.getElementById('blogId').value = blog._id;
                                    document.getElementById('blogTitle').value = blog.title;
                                    document.getElementById('blogCategory').value = blog.category;
                                    document.getElementById('blogDate').value = blog.date.split('T')[0];
                                    document.getElementById('blogDescription').value = blog.description;
                                    document.getElementById('blogContent').value = blog.content;
                                    document.getElementById('blogMetaTitle').value = blog.metaTitle;
                                    document.getElementById('blogMetaDescription').value = blog.metaDescription;
                                    document.getElementById('blogAuthor').value = blog.author;
                                    document.getElementById('blogIsPublished').checked = blog.isPublished;
                                    document.getElementById('blogImage').required = false; // Not required for edit
                                    document.getElementById('blogFormModal').style.display = 'block';
                                }

                                function closeBlogForm() {
                                    document.getElementById('blogFormModal').style.display = 'none';
                                }

                                document.getElementById('blogForm').addEventListener('submit', async function(e) {
                                    e.preventDefault();
                                    const formData = new FormData(this);
                                    const blogId = document.getElementById('blogId').value;
                                    
                                    try {
                                        const url = blogId ? `/admin/blogs/${blogId}` : '/admin/blogs';
                                        const method = blogId ? 'PUT' : 'POST';
                                        
                                        const response = await fetch(url, {
                                            method: method,
                                            body: formData
                                        });

                                        if (!response.ok) {
                                            throw new Error('Failed to save blog');
                                        }

                                        window.location.reload();
                                    } catch (error) {
                                        console.error('Error:', error);
                                        alert('Failed to save blog');
                                    }
                                });

                                async function toggleBlogStatus(blogId, checkbox) {
                                    try {
                                        const response = await fetch(`/admin/blogs/${blogId}/toggle-status`, {
                                            method: 'PATCH',
                                            headers: {
                                                'Content-Type': 'application/json'
                                            }
                                        });
                                        
                                        if (!response.ok) {
                                            throw new Error('Failed to toggle status');
                                        }
                                        
                                        const data = await response.json();
                                        checkbox.checked = data.isPublished;
                                    } catch (error) {
                                        console.error('Error:', error);
                                        alert('Failed to update blog status');
                                        checkbox.checked = !checkbox.checked;
                                    }
                                }

                                async function deleteBlog(blogId) {
                                    if (!confirm('Are you sure you want to delete this blog?')) {
                                        return;
                                    }

                                    try {
                                        const response = await fetch(`/admin/blogs/${blogId}`, {
                                            method: 'DELETE'
                                        });

                                        if (!response.ok) {
                                            throw new Error('Failed to delete blog');
                                        }

                                        document.querySelector(`[data-blog-id="${blogId}"]`).remove();
                                    } catch (error) {
                                        console.error('Error:', error);
                                        alert('Failed to delete blog');
                                    }
                                }

                                document.getElementById('searchForm').addEventListener('submit', function(e) {
                                    e.preventDefault();
                                    const searchTerm = document.getElementById('searchInput').value.toLowerCase();
                                    const blogs = document.querySelectorAll('#blogsList li');
                                    
                                    blogs.forEach(blog => {
                                        const title = blog.querySelector('.body-title-2').textContent.toLowerCase();
                                        const description = blog.querySelector('.body-text').textContent.toLowerCase();
                                        
                                        if (title.includes(searchTerm) || description.includes(searchTerm)) {
                                            blog.style.display = '';
                                        } else {
                                            blog.style.display = 'none';
                                        }
                                    });
                                });
                                </script>
