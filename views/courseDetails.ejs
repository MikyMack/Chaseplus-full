    <section class="blog-details">
      <div class="container">
        <div class="row justify-content-center">
          <div class="col-xl-12 col-lg-7">
            <div class="blog-details__content">
              <div class="blog-details__img">
                <img src="<%= course.image %>" alt="<%= course.title %>" />
              </div>
              <!-- details-image -->
              <div class="blog-details__meta">
                <div class="blog-details__meta__date">
                  <i class="icon-clock"></i>Course Duration : <%= course.duration %>
                </div>
              </div>
              <!-- /.details-meta -->
              <h3 class="blog-details__title">
                <%= course.title %>
              </h3>
              <!-- details-title -->
              <p class="blog-details__text">
                 <%= course.description %>
              </p>

              <h4 class="blog-details__subtitle">Course Highlights:</h4>
              <ul class="blog-details__list">
                <% course.highlights.forEach(function(highlight) { %>
                  <li><%= highlight %></li>
                <% }); %>
              </ul>

              <h4 class="blog-details__subtitle">What You'll Learn:</h4>
              <p class="blog-details__text">
                The curriculum covers essential topics including:
                <% course.whatYoullLearn.forEach(function(point) { %>
                  <br>- <%= point %>
                <% }); %>
              </p>

              <h4 class="blog-details__subtitle">Career Opportunities:</h4>
              <p class="blog-details__text">
                Graduates can pursue roles such as:
                <% course.careerOpportunities.forEach(function(opportunity) { %>
                  <br>- <%= opportunity %>
                <% }); %>
              </p>

              <h4 class="blog-details__subtitle">Why Choose This Course:</h4>
              <p class="blog-details__text">
                <% course.whyChooseThisCourse.forEach(function(reason) { %>
                  <br>- <%= reason %>
                <% }); %>
              </p>
            </div>
            <!-- details-content -->

            <div class="blog-details__comment-form">
              <h3 class="blog-details__comment-form__title">
                Have any Questions? Let's Talk!
              </h3>
              <form action="https://bracketweb.com/eduact-html//inc/sendemail.php" class="contact-form-validated"
                novalidate="novalidate">
                <div class="row">
                  <div class="col-xl-6">
                    <div class="blog-details__comment-form__input-box">
                      <input type="text" placeholder="Your name" name="name" />
                    </div>
                  </div>
                  <div class="col-xl-6">
                    <div class="blog-details__comment-form__input-box">
                      <input type="email" placeholder="Email address" name="email" />
                    </div>
                  </div>
                </div>
                <div class="row">
                  <div class="col-xl-12">
                    <div class="blog-details__comment-form__input-box">
                      <textarea name="message" placeholder="Write  a comment"></textarea>
                    </div>
                    <button type="submit" class="eduact-btn eduact-btn-second">
                      <span class="eduact-btn__curve"></span>Submit
                      <i class="icon-arrow"></i>
                    </button>
                  </div>
                </div>
              </form>
              <div class="result"></div>
            </div>
            <!-- details-comment-form -->
          </div>
        </div>
      </div>
    </section>
